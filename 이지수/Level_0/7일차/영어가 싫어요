class Solution {
    public long solution(String numbers) {
         long answer = 0;
        String num = numbers
        .replace("zero","0")
        .replace("two","2")
        .replace("one","1")
        .replace("three","3")
        .replace("four","4")
        .replace("five","5")
        .replace("six","6")
        .replace("seven","7")
        .replace("eight","8")
        .replace("nine","9");

        answer = Long.parseLong(num);
        return answer;
    }
}
