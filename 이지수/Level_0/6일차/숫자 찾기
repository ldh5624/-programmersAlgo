class Solution {
    public int solution(int num, int k) {
         int answer = -1;
        String numS = String.valueOf(num);
        char[] numC = numS.toCharArray();
        for(int i=0; i<numC.length; i++){
            if(numC[i]-'0' == k){
                answer = i+1;
                break;
            }
        }
        return answer;
    }
}
