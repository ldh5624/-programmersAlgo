class Solution {
    public String solution(String my_string, int n) {
        String answer = "";
        String str[] = new String[my_string.length()];

        for(int i = 0; i < my_string.length(); i++){
            str[i] = my_string.substring(i, i+1);
            for(int j = 0; j < n; j++){
                answer += str[i];
            }
        }
        return answer;
    }
}